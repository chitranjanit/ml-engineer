    "#Example 4\n",
    "import numpy as np\n",
    "a = np.arange(8).reshape(2,4)\n",
    "print('The original array is:')\n",
    "print(a)\n",
    "print('\\n')\n",
    "\n",
    "\n",
    "print('After applying revel function:')\n",
    "print (a.ravel())\n",
    "\n",
    "print('\\n')\n",
    "print('applying revel function in F-style ordering:')\n",
    "print(a.ravel(order='F'))"
   ]
